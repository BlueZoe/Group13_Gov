AAA,QAAA;AAAA,UAAA,WAAA,SAAA,WAAA;;AACA,UAAA,QAAA,KAAA,CAAA,EAAA;AACA,eAAA,uCAAA;AACA;AACA,KA5EA;AA6EA,IAAA,cA7EA,4BA6EA;AACA,aAAA,wBAAA;AACA,KA/EA;;AAgFA;;;AAGA,IAAA,QAnFA;AAAA;AAAA;AAAA,gDAmFA,KAnFA,EAmFA,IAnFA;AAAA;AAAA;AAAA;AAAA;AAAA;AAoFA,gBAAA,KApFA,GAoFA,IApFA;AAqFA,qBAAA,WAAA,GAAA,IAAA;AArFA;AAAA,uBAsFA,KAAA,IAAA,CAAA,eAAA,CAAA,KAAA,CAtFA;;AAAA;AAsFA,gBAAA,MAtFA;;AAuFA,oBAAA,MAAA,CAAA,IAAA,IAAA,CAAA,EAAA;AACA,uBAAA,YAAA,GAAA,MAAA,CAAA,IAAA;AACA,iBAFA,MAEA;AACA,uBAAA,QAAA,CAAA,KAAA,CAAA,MAAA,CAAA,GAAA,IAAA,MAAA;AACA;;AA3FA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;;AAAA;AAAA;AAAA;;AAAA;AAAA;AA6FA;AACA,IAAA,gBA9FA,4BA8FA,KA9FA,EA8FA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,KAAA;AACA,WAAA,KAAA,GAAA,KAAA;AACA,WAAA,UAAA;AACA,KAlGA;AAmGA,IAAA,mBAnGA,+BAmGA,KAnGA,EAmGA;AACA,UAAA,MAAA,GAAA,CAAA,KAAA,GAAA,CAAA,IAAA,KAAA,KAAA;AACA,WAAA,MAAA,GAAA,MAAA;AACA,WAAA,UAAA;AACA,KAvGA;AAwGA,IAAA,iBAxGA,6BAwGA,KAxGA,EAwGA;AACA,WAAA,MAAA,GAAA,KAAA;AACA,WAAA,WAAA;AACA,KA3GA;AA4GA,IAAA,oBA5GA,gCA4GA,KA5GA,EA4GA;AACA,UAAA,MAAA,GAAA,CAAA,KAAA,GAAA,CAAA,IAAA,KAAA,MAAA;AACA,WAAA,OAAA,GAAA,MAAA;AACA,WAAA,WAAA;AACA,KAhHA;AAiHA,IAAA,GAjHA;AAAA;AAAA;AAAA,gDAiHA,IAjHA;AAAA;AAAA;AAAA;AAAA;AAAA;AAkHA,gBAAA,EAlHA,GAkHA,IAlHA;AAAA;AAAA,uBAmHA,KAAA,IAAA,CAAA,OAAA,CAAA,EAAA,CAnHA;;AAAA;AAmHA,gBAAA,MAnHA;AAoHA,qBAAA,UAAA;AACA,qBAAA,UAAA;;AArHA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;;AAAA;AAAA;AAAA;;AAAA;AAAA;AAAA,GA9BA;AAsJA,EAAA,QAAA,EAAA;AACA;AACA,IAAA,aAFA,2BAEA;AACA,UAAA,GAAA,GAAA,YAAA,CAAA,OAAA,CAAA,SAAA,KAAA,KAAA,OAAA;AACA,UAAA,GAAA,GAAA,IAAA,CAAA,KAAA,CAAA,KAAA,UAAA,GAAA,GAAA,CAAA;;AACA,UAAA,KAAA,UAAA,GAAA,GAAA,GAAA,GAAA,EAAA;AACA,eAAA,EAAA,GAAA;AACA,OAFA,MAEA;AACA,eAAA,GAAA;AACA;AACA;AAVA;AAtJA,CAAA","sourcesContent":["<template>\n  <div class=\"index\">\n    <div class=\"title\">\n      <span>异常事件</span>\n      <span class=\"date\">坪山区人民政府</span>\n      <!-- <div class=\"setting\" title=\"全局设置\">\n        <i class=\"el-icon-setting\"></i>\n        <span>设置</span>\n      </div>-->\n    </div>\n    <!-- 异常事件 -->\n    <section class=\"s1\">\n      <!-- 置顶 -->\n      <el-row>\n        <el-col :span=\"18\" :xs=\"22\">\n          <div class=\"msg_list\" v-if=\"topData.length > 0\">\n            <msg v-for=\"item in topData\" :data=\"item\" :key=\"item['主键'] + Math.random()*100\" />\n          </div>\n          <el-alert v-else title=\"暂无置顶信息\" type=\"info\" effect=\"dark\"></el-alert>\n        </el-col>\n      </el-row>\n      <!-- 当日 -->\n      <el-row>\n        <el-col :span=\"18\" :xs=\"22\">\n          <div class=\"msg_list\">\n            <msg :data=\"showData\" />\n          </div>\n        </el-col>\n      </el-row>\n    </section>\n    <!-- 其他消息 -->\n    <section class=\"s3\">\n      <el-row>\n        <el-col :span=\"18\" :xs=\"22\">\n          <div class=\"table\">\n            <el-table\n              size=\"mini\"\n              :cell-style=\"tableCellStyle\"\n              :header-cell-style=\"tableHeaderColor\"\n              :row-style=\"tableRowStyle\"\n              :data=\"allTableData\"\n              style=\"width: 100%\"\n            >\n              <el-table-column prop=\"统计时间\" label=\"时间\" width=\"180\"></el-table-column>             \n              <el-table-column prop=\"所属社区\" label=\"所属社区\" width=\"140\"></el-table-column>\n              <el-table-column prop=\"问题来源\" label=\"问题来源\"width=\"100\"></el-table-column>\n              <el-table-column prop=\"小类名称\" label=\"小类名称\" width=\"180\"></el-table-column>\n              <el-table-column prop=\"处置部门\" label=\"处置部门\"></el-table-column>\n              <el-table-column fixed=\"right\" label=\"操作\" width=\"100\">\n                <template slot-scope=\"scope\">\n                  <el-button @click=\"top(scope.row)\" type=\"success\" size=\"mini\">置顶</el-button>\n                </template>\n              </el-table-column>\n            </el-table>\n\n            <div class=\"pages\">\n              <el-pagination\n                :page-sizes=\"[5,10,15,20,25]\"\n                :page-size=\"limit\"\n                background\n                @size-change=\"handleSizeChange\"\n                @current-change=\"handleCurrentChange\"\n                layout=\"prev, sizes,pager, next\"\n                :total=\"len\"\n              ></el-pagination>\n            </div>\n          </div>\n        </el-col>\n      </el-row>\n    </section>\n    <!-- 多人投诉 -->\n    <section class=\"s2\">\n      <el-row>\n        <el-col :span=\"18\" :xs=\"22\">\n          <!-- <msg v-for=\"item in manyData\" type=\"s2\" :key=\"item.data['主键']\" :data=\"item.data\" /> -->\n          <el-table\n            size=\"mini\"\n            :cell-style=\"tableCellStyle\"\n            :header-cell-style=\"tableHeaderColor\"\n            :row-style=\"tableRowStyle\"\n            :data=\"tableData\"\n            style=\"width: 100%\"\n          >\n            <el-table-column prop=\"所属街道\" label=\"街道\" width=\"180\"></el-table-column>\n            <el-table-column prop=\"所属社区\" label=\"社区\" width=\"180\"></el-table-column>\n            <el-table-column prop=\"问题性质\" label=\"问题性质\" width=\"180\"></el-table-column>\n            <el-table-column prop=\"num\" label=\"事件总数\"></el-table-column>\n            <el-table-column align=\"right\">\n              <template slot-scope=\"scope\">\n                <el-button\n                  size=\"mini\"\n                  type=\"primary\"\n                  @click=\"showInfo(scope.$index, scope.row)\"\n                >详细信息</el-button>\n              </template>\n            </el-table-column>\n          </el-table>\n          <div class=\"pages\">\n            <el-pagination\n              :page-sizes=\"[5,10,15,20,25]\"\n              :page-size=\"limit2\"\n              background\n              @size-change=\"handleSizeChange2\"\n              @current-change=\"handleCurrentChange2\"\n              layout=\"prev, sizes,pager, next\"\n              :total=\"len2\"\n            ></el-pagination>\n          </div>\n        </el-col>\n      </el-row>\n    </section>\n    <!-- 多人投诉消息展示 -->\n    <div class=\"dialog\">\n      <el-dialog title=\"详细信息\" :close-on-click-modal=\"false\" :visible.sync=\"showAllInfo\">\n        <el-table size=\"mini\" :data=\"showInfoData\" border style=\"width: 100%\">\n          <el-table-column prop=\"统计时间\" label=\"时间\" width=\"160\"></el-table-column>\n          <el-table-column prop=\"问题来源\" label=\"问题来源\" width=\"100\"></el-table-column>\n          <el-table-column prop=\"问题类型\" label=\"问题类型\" width=\"100\"></el-table-column>\n          <el-table-column prop=\"小类名称\" label=\"小类名称\"width=\"100\"></el-table-column>\n          <el-table-column prop=\"处置部门\" label=\"处置部门\"></el-table-column>\n        </el-table>\n      </el-dialog>\n    </div>\n  </div>\n</template>\n\n\n<script>\nimport msg from \"@/components/message.vue\";\nexport default {\n  components: {\n    msg\n  },\n  data() {\n    return {\n      nowData: [],\n      nowDataLen: 0,\n      showLen: 1, // 每页显示多少条\n      showData: {},\n      topData: [], // 置顶数据\n      manyData: {}, // 多人投诉数据\n      allTableData: [], // 10.30前数据\n      tableData: [], // 表格数据\n      showInfoData: [], //显示详细信息\n      showAllInfo: false, // 显示控制\n      offset: 0,\n      limit: 5,\n      len: 0,\n      offset2: 0,\n      limit2: 5,\n      len2: 0\n    };\n  },\n  created() {\n    this.getNowInfo();\n    this.getTopData();\n    this.getManyData();\n    this.beforeData();\n  },\n  methods: {\n    // 获取当天实时数据\n    async getNowInfo() {\n      const result = await this.$api.getNowInfo();\n      if (result && result.code == 0) {\n        this.nowData = result.data;\n        this.nowDataLen = result.data.length;\n        let len = localStorage.getItem(\"showLen\") || this.showLen;\n        this.showData = result.data[result.data.length - 1];\n        console.log(this.showData);\n        setTimeout(() => {\n          this.getNowInfo();\n        }, 2000);\n      }\n    },\n    async beforeData() {\n      const result = await this.$api.getBeforeData({\n        offset: this.offset,\n        limit: this.limit\n      });\n      if (result.code == 0) {\n        this.len = result.data.len;\n        this.allTableData = result.data.data;\n      } else {\n        this.$message.error(result.msg);\n      }\n    },\n    async getTopData() {\n      const result = await this.$api.getTopData();\n      if (result && result.code == 0) {\n        this.topData = result.data;\n      }\n    },\n    async getManyData() {\n      const result = await this.$api.getManyData({\n        offset: this.offset2,\n        limit: this.limit2\n      });\n      if (result && result.code == 0) {\n        this.manyData = result.data.data;\n        this.tableData = result.data.data;\n        // let data = result.data.data;\n        // let datas = [];\n        // for (let prop in data) {\n        //   datas.push({\n        //     j: data[prop].data[\"所属街道\"],\n        //     s: data[prop].data[\"所属社区\"],\n        //     l: data[prop].data[\"问题来源\"],\n        //     x: data[prop].data[\"问题类型\"],\n        //     num: data[prop].items.length,\n        //     items: data[prop].items\n        //   });\n        // }\n        // this.tableData = datas;\n        this.len2 = result.data.len;\n      }\n    },\n    // 信息混动时间\n    changeNowInfo(index) {\n      // let len = localStorage.getItem(\"showLen\") || this.showLen;\n      // let start = len * index;\n      // let end = start + len;\n      // this.showData = this.nowData.slice(start, end);\n      // if (index == 0) {\n      //   this.getNowInfo();\n      //   this.getTopData();\n      // }\n    },\n    tableRowStyle({ row, index }) {\n      return \"background-color: #0c141f;border:none;\";\n    },\n    // 标题设置\n    tableHeaderColor({ row, column, rowIndex, columnIndex }) {\n      if (rowIndex === 0) {\n        return \"background-color: #0c141f;color:#fff;\";\n      }\n    },\n    tableCellStyle() {\n      return \"border:none;color:#eee\";\n    },\n    /**\n     * 获取详细信息\n     */\n    async showInfo(index, data) {\n      let items = data\n      this.showAllInfo = true;\n      const result = await this.$api.getMonyDataInfo( items );\n      if (result.code == 0) {\n        this.showInfoData = result.data;\n      } else {\n        this.$message.error(result.msg || \"系统出错\");\n      }\n    },\n    // 获取数量改变\n    handleSizeChange(index) {\n      console.log(index);\n      this.limit = index;\n      this.beforeData();\n    },\n    handleCurrentChange(index) {\n      let offset = (index - 1) * this.limit;\n      this.offset = offset;\n      this.beforeData();\n    },\n    handleSizeChange2(index) {\n      this.limit2 = index;\n      this.getManyData();\n    },\n    handleCurrentChange2(index) {\n      let offset = (index - 1) * this.limit2;\n      this.offset2 = offset;\n      this.getManyData();\n    },\n    async top(data) {\n      let id = data;\n      const result = await this.$api.topData( id );\n      this.beforeData();\n      this.getTopData();\n    }\n  },\n  computed: {\n    // 页数计算\n    nowDataForLen() {\n      let len = localStorage.getItem(\"showLen\") || this.showLen;\n      let num = Math.round(this.nowDataLen / len);\n      if (this.nowDataLen / len > num) {\n        return ++num;\n      } else {\n        return num;\n      }\n    }\n  }\n};\n</script>\n\n<style lang=\"scss\" scoped>\n.index {\n  background-color: #0c141f;\n  .title {\n    position: relative;\n    text-align: center;\n    color: #fff;\n    padding: 20px 20px;\n    margin: 10px 10px 0px 0px;\n    span {\n      font-size: 28px;\n      padding: 10px 0;\n    }\n    .setting {\n      cursor: pointer;\n      position: absolute;\n      top: 0;\n      right: 20px;\n      font-size: 28px;\n      display: flex;\n      align-items: center;\n      justify-content: center;\n      span {\n        font-size: 17px;\n      }\n    }\n    .date {\n      position: absolute;\n      bottom: 0px;\n      left: 60%;\n      margin-left: 100px;\n      font-size: 13px;\n    }\n  }\n}\nsection {\n  width: 100%;\n  min-height: 100px;\n  .el-row {\n    display: flex;\n    justify-content: center;\n  }\n  .el-col {\n    padding: 15px;\n    box-shadow: 0 2px 12px 0 rgba(255, 255, 255, 0.1);\n  }\n  div.table {\n    div {\n      background-color: #0c141f;\n    }\n  }\n}\n</style>\n<style lang=\"scss\">\nsection .el-table--enable-row-hover .el-table__body tr:hover > td {\n  background-color: #fefefe !important;\n}\nsection .el-table--enable-row-hover .el-table__body tr:hover > td > div {\n  color: #0c141f !important;\n}\n.el-table .el-table__body tbody tr.el-table__row.hover-row {\n  background-color: #fefefe !important;\n}\n.el-table .el-table__body tbody tr.el-table__row.hover-row > td > div {\n  color: #0c141f !important;\n}\n</style>"],"sourceRoot":"src/views"}]}